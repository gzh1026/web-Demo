<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.dao.User" table="user" schema="bagevent_test">
        <id name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </id>
        <property name="userName">
            <column name="user_name" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="cellphone">
            <column name="cellphone" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="salt">
            <column name="salt" sql-type="varchar(255)"/>
        </property>
        <property name="state">
            <column name="state" sql-type="int(2)"/>
        </property>
        <property name="createTime">
            <column name="create_time" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>