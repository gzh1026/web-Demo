<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.dao.Userloginlog" table="userloginlog" schema="bagevent_test">
        <id name="loginId">
            <column name="login_id" sql-type="int(11)"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </property>
        <property name="loginTime">
            <column name="login_time" sql-type="datetime"/>
        </property>
        <property name="loginIp">
            <column name="login_ip" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>